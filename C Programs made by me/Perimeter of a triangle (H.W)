#include<stdio.h>

int main(){
 
    float a,b,c;
    
    printf("enter a");
    scanf("%f",&a);//taking input a 

    printf("enter b");
    scanf("%f",&b);// taking input b

    printf("enter c");
     scanf("%f",&c);//taking input c

    printf("Perimeter of the triangle is : %f",a+b+c);//output perimeter of triangle as a+b+c

    return 0;
}